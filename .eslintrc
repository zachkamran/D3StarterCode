{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "uber-es2015"
  ],
  "rules": {
    "consistent-return": 0,
    "max-len": [1, 110, 4],
    "max-params": ["error", 6],
    "object-curly-spacing": 0,
    "babel/object-curly-spacing": 2,
    "no-loops/no-loops": 2
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "no-loops"
  ]
}
